KDIR=${KDIR:-/lib/modules/`uname -r`/build}

make -C ${KDIR} M=$(pwd) modules > /dev/null 2>&1

if [ $? -eq 0 ] ; then
	echo "#define HAVE_BIO_SET_OP_ATTRS 1"
else
	echo "#define HAVE_BIO_SET_OP_ATTRS 0"
fi
